{
  "name": "cv-creator-webapp",
  "version": "0.0.1-alpha",
  "scripts": {
    "cypress-open": "cypress open",
    "cypress-run": "cypress run",
    "ng": "ng",
    "serve-prod": "ng serve --configuration=production  --source-map --port=4200 --optimization=true",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "test-ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "test-code-coverage": "ng test --code-coverage --no-watch --no-progress --browsers=ChromeHeadless",
    "karma": "ng test --code-coverage --no-watch --no-progress --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "gitlab:build": "docker build -t registry.gitlab.com/chandafrz/cv-creator-webapp .",
    "gitlab:push": "docker push registry.gitlab.com/chandafrz/cv-creator-webapp",
    "docker:build": "docker build --rm -t cv-creator-webapp --build-arg configuration='production' .",
    "docker:run": "docker run -p 4201:80 cv-creator-webapp",
    "docker:it": "npm run docker:build && npm run docker:run",
    "sonar:start": "/Applications/sonarqube/bin/macosx-universal-64/sonar.sh start",
    "sonar:stop": "/Applications/sonarqube/bin/macosx-universal-64/sonar.sh stop",
    "sonar:status": "/Applications/sonarqube/bin/macosx-universal-64/sonar.sh status",
    "sonar": "/Applications/sonar-scanner/bin/sonar-scanner",
    "heroku:push": "heroku container:push web --app cv-creator-webapp",
    "heroku:release": "heroku container:release web --app cv-creator-webapp",
    "heroku:it": "npm run heroku:push && npm run heroku:release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.5",
    "@angular/cdk": "^8.1.3",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~8.2.5",
    "@angular/forms": "~8.2.5",
    "@angular/material": "^8.1.3",
    "@angular/platform-browser": "~8.2.5",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/pwa": "^0.802.0",
    "@angular/router": "~8.2.5",
    "@angular/service-worker": "~8.2.5",
    "@auth0/angular-jwt": "^3.0.0",
    "@ngrx/data": "^8.3.0",
    "@ngrx/effects": "^8.3.0",
    "@ngrx/entity": "^8.3.0",
    "@ngrx/schematics": "^8.3.0",
    "@ngrx/store": "^8.3.0",
    "@ngrx/store-devtools": "^8.3.0",
    "angular2-cookie-law": "^7.0.1",
    "bootstrap": "^4.3.1",
    "hammerjs": "^2.0.8",
    "memo-decorator": "^1.0.2",
    "ngx-device-detector": "^1.3.11",
    "ngx-logger": "^4.0.4",
    "ngx-textarea-autosize": "^2.0.3",
    "rxjs": "~6.5.3",
    "subsink": "^1.0.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.0",
    "@angular-devkit/build-optimizer": "^0.1302.1",
    "@angular/cli": "~8.3.0",
    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ts-loader": "6.0.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.5.3"
  },
  "excluded": {
    "cypress": "^3.4.1",
    "@briebug/cypress-schematic": "^2.0.0",
    "@cypress/webpack-preprocessor": "4.1.0"
  }
}